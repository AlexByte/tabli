{
  "name": "tabli",
  "version": "0.9.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonycourtney/tabli.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chrome-promise": "^3.0.2",
    "escape-string-regexp": "^1.0.5",
    "events": "^3.0.0",
    "flow-bin": "^0.75.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "object-assign": "^4.1.1",
    "oneref": "^0.3.2",
    "popper": "^1.0.1",
    "react": "^16.4.1",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.4.1",
    "reactstrap": "^6.2.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.1.1",
    "difflet": "^1.0.1",
    "ekko-lightbox": "^5.3.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "filesize": "^3.6.1",
    "flow-interfaces-chrome": "^0.4.0",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^4.3.1",
    "jsdom": "^11.11.0",
    "json-loader": "^0.5.7",
    "memwatch-next": "^0.3.0",
    "pre-commit": "^1.2.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "sinon": "^6.0.1",
    "standard": "^11.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8"
  },
  "scripts": {
    "build:dir": "mkdirp build && mkdirp build/css && mkdirp build/js && mkdirp build/fonts && mkdirp build/images && mkdirp build/testData",
    "build:bootstrap": "cp -r node_modules/bootstrap/dist/* build",
    "build:jquery": "cp -r node_modules/jquery/dist/* build/js",
    "build:fa": "cp -r node_modules/font-awesome/css/* build/css && cp -r node_modules/font-awesome/fonts/* build/fonts",
    "build:html": "cp -r src/css/* build/css && cp -r src/html/* build",
    "build:images": "cp -r src/images/* src/assets/*.png build/images",
    "build:popper": "cp -r node_modules/popper.js/dist/* build/js",
    "build:testData": "cp -r test/*.json build/testData",
    "build:manifest": "cp src/manifest.json build",
    "build-assets": "npm run build:dir && npm run build:jquery && npm run build:bootstrap && npm run build:popper && npm run build:fa && npm run build:html && npm run build:images && npm run build:testData && npm run build:manifest",
    "build-dev": "npm run build-assets && webpack --mode development",
    "build-prod": "npm run build-assets && webpack --mode production",
    "clean": "rm -rf ./build",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "leaktest": "babel-node --expose-gc --presets es2015,react,stage-3 ./test/leakTest.js | tap-spec",
    "lint": "eslint .",
    "lintfix": "eslint --fix .",
    "test": "eslint . && babel-node ./test/runAllTests.js | tap-spec",
    "watch": "webpack --mode development --watch",
    "webpack-analyze": "webpack --mode production --profile --json > stats.json; ./node_modules/.bin/webpack-bundle-analyzer stats.json ./build/js"
  },
  "standard": {
    "globals": [
      "chrome",
      "performance"
    ],
    "ignore": [
      "webpack.config.js"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
